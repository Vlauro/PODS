    <!-- http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P94_has_created_(was_created_by) -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P94_has_created_(was_created_by)">
