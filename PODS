    <!-- http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P33_used_specific_technique_(was_used_by) -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P33_used_specific_technique_(was_used_by)">
