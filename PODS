    <!-- https://photogrammetry.altervista.org/items/show/151#F64_Modelling_Description -->

    <owl:Class rdf:about="https://photogrammetry.altervista.org/items/show/151#F64_Modelling_Description">
        <rdfs:subClassOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/E62_String#E62_String"/>
