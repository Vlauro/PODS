    <!-- https://photogrammetry.altervista.org/items/show/265#Y94:_has_observed_number_of_points -->

    <owl:DatatypeProperty rdf:about="https://photogrammetry.altervista.org/items/show/265#Y94:_has_observed_number_of_points">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P40_observed_dimension_(was_observed_in)"/>
        <rdfs:domain rdf:resource="https://photogrammetry.altervista.org/items/show/136#F50_Point_Cloud_Measurement"/>
