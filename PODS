    <!-- http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P9_consists_of_(forms_part_of) -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2024/9/PODS_1#P9_consists_of_(forms_part_of)">
