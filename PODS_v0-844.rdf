    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#A1_had_raw_data_as_output -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#A1_had_raw_data_as_output">
        <rdfs:subPropertyOf rdf:resource="http://www.ics.forth.gr/isl/CRMdig#L11_had_output_(was_output_of)"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F2_PG_Acquisition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F25_Acquisition_Raw_Data"/>
        <rdfs:comment xml:lang="en">This property links an acquisition activity (F2) to its direct digital output (F25), typically the raw data generated by the capture device. In photogrammetry, this includes the original photographs or image sets produced for subsequent processing steps.</rdfs:comment>
        <rdfs:label xml:lang="en">Y158 had raw data as output</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y122_was_influenced_by_eidotype -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y122_was_influenced_by_eidotype">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P15_was_influenced_by"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F2_PG_Acquisition"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F40_Eidotype"/>
        <rdfs:comment xml:lang="en">This property links the Acquisition (F2) process with an Eidotype (F40), indicating that the acquisition was influenced or guided by the eidotype. An eidotype refers to an idealized or conceptual representation, such as a blueprint or reference model, which informs the acquisition process by providing a visual or theoretical guide for capturing the data.</rdfs:comment>
        <rdfs:label xml:lang="en">Y122 was influenced by eidotype</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y157_had_raw_data_as_input -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y157_had_raw_data_as_input">
        <rdfs:subPropertyOf rdf:resource="http://www.ics.forth.gr/isl/CRMdig#L10_had_input_(was_input_of)"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F25_Acquisition_Raw_Data"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F46_Matching"/>
        <rdfs:comment xml:lang="en">This property links a set of acquisition raw data (F25) to the first phase of processing (F46), where the data is used as input for matching operations. In photogrammetry, this typically refers to the raw photographs or depth data being processed to generate a point cloud.
 It ensures the traceability of the original input dataset that initiates the computation pipeline.</rdfs:comment>
        <rdfs:label xml:lang="en">Y157 had raw data as input</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y161_is_point_cloud_representation_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y161_is_point_cloud_representation_of">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P67_refers_to"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Archeological_Finding_Record"/>
        <rdfs:comment xml:lang="en">This property establishes a semantic connection between a generic point cloud and the corresponding archaeological finding it represents. It signifies that the digital representation was generated specifically to document or visualize a physical archaeological asset, supporting spatial interpretation and research.</rdfs:comment>
        <rdfs:label xml:lang="en">Y161 is point cloud representation of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y162_is_related_to_a_place -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y162_is_related_to_a_place">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F42_Place_Of_Acquisition"/>
        <rdfs:comment xml:lang="en">This property links a point cloud to the geographic location where the acquisition took place. It supports the spatial contextualization of digital assets and should preferably be expressed through persistent geographic URIs such as those from OpenStreetMap or GeoNames.</rdfs:comment>
        <rdfs:label xml:lang="en">Y162 is related to a place</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y163_was_produced_thanks_to_acquisition_device -->

