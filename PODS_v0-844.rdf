        <rdfs:subPropertyOf rdf:resource="http://www.ics.forth.gr/isl/CRMdig#L19i_is_stored_on"/>
        <rdfs:comment xml:lang="en">This property connects a point cloud with the specific acquisition device that enabled its creation. It documents the material instrumentality of data generation, aiding in reproducibility and analysis of quality parameters.</rdfs:comment>
        <rdfs:label xml:lang="en">Y163 was produced thanks to acquisition device</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y164_has_eidotype_as_production_note -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y164_has_eidotype_as_production_note">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P165_incorporates"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F40_Eidotype"/>
        <rdfs:comment xml:lang="en">This property links a point cloud to an eidotype, a structured annotation that conveys production-level metadata such as noise levels, scanning anomalies, lighting issues, or subjective choices made during modeling.</rdfs:comment>
        <rdfs:label xml:lang="en">Y164 has eidotype as production note</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y165_created_by_type_of_acquisition -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y165_created_by_type_of_acquisition">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P2_has_type#P2_has_type"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F17_Acquisition_Trajectory_Type"/>
        <rdfs:comment xml:lang="en">This property specifies the type of acquisition trajectory followed during data capture, characterizing the operational geometry of the acquisition process (e.g., spiral, linear, circular), and facilitating comparisons among acquisition strategies.</rdfs:comment>
        <rdfs:label xml:lang="en">Y165 created by type of acquisition</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y166_raw_data_used_as_base_of_creation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y166_raw_data_used_as_base_of_creation">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P148_has_component"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F25_Acquisition_Raw_Data"/>
        <rdfs:comment xml:lang="en">This property identifies the raw data from which the point cloud was derived. It supports the documentation of provenance and traceability within the digital workflow.</rdfs:comment>
        <rdfs:label xml:lang="en">Y166 raw data used as base of creation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y167_based_on_type_of_matching -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y167_based_on_type_of_matching">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P2_has_type#P2_has_type"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F21_Matching_Algorithm_Type"/>
        <rdfs:comment xml:lang="en">This property associates a point cloud with the type of image-matching algorithm applied during feature extraction and triangulation. It enables analytical assessment of reconstruction strategies.</rdfs:comment>
        <rdfs:label xml:lang="en">Y167 based on type of matching</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y168_based_on_type_of_densification -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y168_based_on_type_of_densification">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P2_has_type#P2_has_type"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F22_Densification_Algorithm_Type"/>
        <rdfs:comment xml:lang="en">This property records the specific densification algorithm employed to enrich a sparse point cloud. It clarifies the algorithmic basis of the resulting geometry.</rdfs:comment>
        <rdfs:label xml:lang="en">Y168 based on type of densification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y169_was_produced_on_processing_device -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y169_was_produced_on_processing_device">
        <rdfs:subPropertyOf rdf:resource="http://www.ics.forth.gr/isl/CRMdig#L19i_is_stored_on"/>
        <rdfs:comment xml:lang="en">This property links a point cloud to the physical hardware on which the processing was executed. It documents the technical environment that may affect performance or reproducibility.</rdfs:comment>
        <rdfs:label xml:lang="en">Y169 was produced on processing device</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y170_creation_work_carried_out_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#Y170_creation_work_carried_out_by">
        <rdfs:subPropertyOf rdf:resource="http://cidoc-crm.org/cidoc-crm/7.1.3/P52_has_current_owner"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F171_PG_Generic_Point_Cloud"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/vitto/ontologies/2025/4/PODS_08#F38_Processing_Operator"/>
        <rdfs:comment xml:lang="en">This property identifies the operator responsible for the processing stage that generated the point cloud. It supports accountability and contextualization of human intervention in the workflow.</rdfs:comment>
        <rdfs:label xml:lang="en">Y170 creation work carried out by</rdfs:label>
    </owl:ObjectProperty>
    


